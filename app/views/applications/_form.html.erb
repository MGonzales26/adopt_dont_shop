<%= form_with model: @application, method: method, url: url, local: true do |f| %>  
    <%= f.label :name, "Your Name:" %><br>
    <%=  f.text_field :name %><br>
    <%= f.label :street_address, "Street Address:" %><br>
    <%=  f.text_field :street_address %><br>
    <%= f.label :city, "City:" %><br>
    <%=  f.text_field :city %><br>
    <%= f.label :state, "State:" %><br>
    <%=  f.text_field :state %><br>
    <%= f.label :zip_code, "Zip Code:" %><br>
    <%=  f.text_field :zip_code %><br>
    <%= f.submit submit %>
  <% end %>